.lunbo{
    height:350rpx;
    overflow:hidden;
    background:#74bee3;
}
.lunbo swiper{
    height:350rpx;
}
.slide_image{
  position:relative;
  display: block;
  height:350rpx;
  width:840rpx;
}
.container{
  background:#f8f8f8; 
}
.menu_list{
  height:200rpx;
  background: #fff;
  border-top:1rpx solid #f2f2f2;
  border-bottom:1rpx solid #f2f2f2;
}
.menu_item{
  width:33.33%;
  height:90rpx;
  float:left;
  position:relative;
  padding-top:110rpx;
  line-height: 60rpx;
  font-size:28rpx;
  text-align: center;
  color:#808080;
}
.menu_photo{
  position:absolute;
  top:40rpx;
  width:60rpx;
  height:60rpx;
  left:50%;
  margin-left:-30rpx;
  border-radius: 60rpx;
  overflow: hidden;
}
.menu_photo image{
  width:60rpx;
  height:60rpx;
}
