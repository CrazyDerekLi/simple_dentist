.container{
  background:#f7f7f7;
  padding-bottom:100rpx;
}
.panel-tab{
  background:#fff;
}
.view-item{
  padding-left:20rpx;
}
.view-item .btn_shoucang{
  position:absolute;
  left:0;
  top:0;
  width:80rpx;
  height:80rpx;
}
.view-item .btn_shoucang image{
  position:absolute;
  width:40rpx;
  height:40rpx;
  top:15rpx;
  left:20rpx;
}
.project_list{
  position:relative;
}
.project_item{
  height:50rpx;
  line-height:50rpx;
  position:relative;
}
.project_item_title{
  width:120rpx;
  position:absolute;
  left:0;
  top:0;
  height:50rpx;
  line-height:50rpx;
  font-size:26rpx;
  color:#7e7b7f;
  text-align: left;
}
.project_item_progress{
  position:absolute;
  left:120rpx;
  right:180rpx;
  width:auto;
  height:10rpx;
  padding:20rpx 0;
}
.project_item_progress .progress_item{
  width:18%;
  height:10rpx;
  background:#f2f2f2;
  float:left;
  margin-right:2%;
}
.project_item_tip{
  width:180rpx;
  position:absolute;
  right:0;
  top:0;
  height:50rpx;
  line-height:50rpx;
  font-size:26rpx;
  color:#ccc;
  text-align: right;
}

.message_list{
  position:relative;
}
.message_list>view:first-child{
  border-top:none;
}
.message_item{
  position:relative;
  padding:20rpx 20rpx 20rpx 120rpx;
  min-height:100rpx;
  border-top:1px solid #f2f2f2;
}
.message_item .message_btn_sound{
  position:absolute;
  width:100rpx;
  height:100rpx;
  left:0;
  top:20rpx;
}
.message_item .message_btn_sound image{
  position:absolute;
  width:100rpx;
  height:100rpx;
  left:0;
  top:0;
}
.message_title,.message_info{
  line-height:40rpx;
  font-size:26rpx;
  color:#7e7b7f;

}
.message_date{
  line-height:50rpx;
  font-size:26rpx;
  color:#ccc;
}

.family_detail_top{
  border-top:1rpx solid #9ed2ea;
  position:relative;
}
.top_blue{
  background: #74bee3;
  height:160rpx;
}
.top_title{
  padding-top:80rpx;
  line-height:80rpx;
  font-size:32rpx;
  text-align: center;
  color:#7f7b7f;
  border-bottom:1rpx solid #f2f2f2;
  background: #fff;
}
.healthy{
  position:absolute;
  width:200rpx;
  height:200rpx;
  top:50rpx;
  left:50%;
  margin-left:-100rpx;
}
.healthy image{
  width:200rpx;
  height:200rpx;
}
.family_detail_box{
  position:relative;
  padding:20rpx 40rpx 20rpx 200rpx;
  background:#fff;
}
.family_detail_photo{
  position:absolute;
  left:20rpx;
  top:50%;
  margin-top:-80rpx;
}
.family_detail_photo image{
  width:160rpx;
  height:160rpx;
}
.family_detail_item{
  height:50rpx;
  line-height:50rpx;
}
.label{
  width:100rpx;
  float:left;
  font-size:26rpx;
  color:#bababa;
  line-height:50rpx;
}
.info{
  float:left;
  font-size:26rpx;
  color:#9e9b9f;
  line-height:50rpx;
}
.family_detail_enter{
  width:12rpx;
  height:24rpx;
  position:absolute;
  top:50%;
  margin-top:-12rpx;
  right:20rpx;
}
.family_detail_enter image{
  width:12rpx;
  height:24rpx;
  position:absolute;
  left:0;
  top:0;
}

.family_detail_btns{
  position:fixed;
  bottom:0;
  height:100rpx;
  border-top:1rpx solid #f2f2f2;
  left:0;
  right:0;
  background:#fff;
}
.family_detail_left_btn{
  background:#fff;
  color:#7e7b7f;
  height:100rpx;
  line-height:100rpx;
  font-size:32rpx;
  width:50%;
  float:left;
  text-align: center;
}
.family_detail_right_btn{
  background:#76daa1;
  color:#fff;
  text-align: center;
  border-left:1rpx solid #f2f2f2;
  margin-left:-1rpx;
  height:100rpx;
  line-height:100rpx;
  font-size:32rpx;
  width:50%;
  float:left;
}