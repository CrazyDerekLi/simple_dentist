.container{
  background:#f7f7f7;
  padding-bottom:100rpx;
}
.panel-tab{
  background:#fff;
}
.view-item{
  padding-left:20rpx;
}
.view-item .btn_shoucang{
  position:absolute;
  left:0;
  top:0;
  width:80rpx;
  height:80rpx;
}
.view-item .btn_shoucang image{
  position:absolute;
  width:40rpx;
  height:40rpx;
  top:15rpx;
  left:20rpx;
}
.project_list{
  position:relative;
}
.project_item{
  height:50rpx;
  line-height:50rpx;
  position:relative;
}
.project_item_title{
  width:120rpx;
  position:absolute;
  left:0;
  top:0;
  height:50rpx;
  line-height:50rpx;
  font-size:26rpx;
  color:#7e7b7f;
  text-align: left;
}
.project_item_progress{
  position:absolute;
  left:120rpx;
  right:180rpx;
  width:auto;
  height:10rpx;
  padding:20rpx 0;
}
.project_item_progress .progress_item{
  width:18%;
  height:10rpx;
  background:#f2f2f2;
  float:left;
  margin-right:2%;
}
.project_item_tip{
  width:180rpx;
  position:absolute;
  right:0;
  top:0;
  height:50rpx;
  line-height:50rpx;
  font-size:26rpx;
  color:#ccc;
  text-align: right;
}

.message_list{
  position:relative;
}
.message_list>view:first-child{
  border-top:none;
}
.message_item{
  position:relative;
  padding:20rpx 20rpx 20rpx 120rpx;
  min-height:100rpx;
  border-top:1px solid #f2f2f2;
}
.message_item .message_btn_sound{
  position:absolute;
  width:100rpx;
  height:100rpx;
  left:0;
  top:20rpx;
}
.message_item .message_btn_sound image{
  position:absolute;
  width:100rpx;
  height:100rpx;
  left:0;
  top:0;
}
.message_title,.message_info{
  line-height:40rpx;
  font-size:26rpx;
  color:#7e7b7f;

}
.message_date{
  line-height:50rpx;
  font-size:26rpx;
  color:#ccc;
}

.family_detail_top{
  border-top:1rpx solid #9ed2ea;
  position:relative;
  background:#74bee3;
}
.top_name{
  background: #74bee3;
  height:40rpx;
  padding-top:230rpx;
  text-align: center;
  font-size:40rpx;
  color:#fff;
  font-weight: bold;
}
.top_title{
  
  line-height:80rpx;
  font-size:28rpx;
  text-align: center;
  color:#fff;
}
.healthy{
  position:absolute;
  width:180rpx;
  height:180rpx;
  top:40rpx;
  left:50%;
  margin-left:-80rpx;
}
.healthy_img_box{
  position:absolute;
  width:160rpx;
  height:160rpx;
  border:10rpx solid #f2f2f2;
  border-radius: 160rpx;
  overflow: hidden;
}
.healthy image{
  width:160rpx;
  height:160rpx;
}
.family_detail_box{
  position:relative;
  padding:20rpx 40rpx 20rpx 40rpx;
  background:#fff;
}
.family_detail_photo{
  position:absolute;
  left:20rpx;
  top:50%;
  margin-top:-80rpx;
}
.family_detail_photo image{
  width:160rpx;
  height:160rpx;
}
.family_detail_item{
  height:50rpx;
  line-height:50rpx;
}
.label{
  width:100rpx;
  float:left;
  font-size:26rpx;
  color:#bababa;
  line-height:50rpx;
}
.info{
  float:left;
  font-size:26rpx;
  color:#9e9b9f;
  line-height:50rpx;
}
.family_detail_enter{
  width:12rpx;
  height:24rpx;
  position:absolute;
  top:50%;
  margin-top:-12rpx;
  right:20rpx;
}
.family_detail_enter image{
  width:12rpx;
  height:24rpx;
  position:absolute;
  left:0;
  top:0;
}

.family_detail_btns{
  position:fixed;
  bottom:0;
  height:100rpx;
  border-top:1rpx solid #f2f2f2;
  left:0;
  right:0;
  background:#fff;
}
