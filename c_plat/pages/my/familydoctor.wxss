.container{
  background:#f8f8f8;
}
.familydoctor_top{
  height:440rpx;
  background:#74bee3;
  border-top:1rpx solid #9ed2ea;
  border-bottom:1rpx solid #f2f2f2;
}
.familydoctor_photo{
  width:160rpx;
  height:160rpx;
  margin:40rpx auto 20rpx auto;
  position:relative;
  border-radius:160rpx;
  border:10rpx solid #f2f2f2;
  overflow: hidden;
}
.familydoctor_photo image{
  position:absolute;
  left:0;
  right:0;
  width:160rpx;
  height:160rpx;
}
.familydoctor_name{
  color:#fff;
  text-align: center;
  height:50rpx;
  font-size:36rpx;
  font-weight: bold;
}
.btn_satus{
  height:36rpx;
  line-height:36rpx;
  font-size:24rpx;
  display: inline-block;
  background:#f28a95;
  padding:0 10rpx;
  border-radius:10rpx;
}
.familydoctor_position{
  text-align: center;
  color:#fff;
  font-size:28rpx;
  line-height:60rpx;
}
.familydoctor_btn{
  width:200rpx;
  height:50rpx;
  font-size:30rpx;
  color:#fff;
  line-height:50rpx;
  border-radius: 50rpx;
  margin:0 auto;
  text-align: center;
  background:#f28a95;
}
.familydoctor_title{
  color:#7f7b7f;
  line-height:100rpx;
  background:#fff;
  padding-left:40rpx;
  font-size:32rpx;
}
.familydoctor_item{
  position:relative;
  height:160rpx;
  padding-left:160rpx;
  font-size:32rpx;
  color:#7f7b7f;
  background: #fff;
  line-height: 160rpx;
  border-top:1rpx solid #f2f2f2;
  border-bottom:1rpx solid #f2f2f2;
  margin-bottom:20rpx;
}
.familydoctor_item_tip{
  position:absolute;
  height:160rpx;
  line-height: 160rpx;
  top:0;
  right:0;
  padding-right:40rpx;
  color:#b5b5b5;
  font-size:28rpx;
}
.familydoctor_item_photo{
  width:120rpx;
  height:120rpx;
  position:absolute;
  top:20rpx;
  left:20rpx;
}
.familydoctor_item_photo_box{
  width:100rpx;
  height:100rpx;
  position:relative;
  border-radius:100rpx;
  border:10rpx solid #f2f2f2;
  overflow: hidden;
}
.familydoctor_item_photo_box image{
  width:100rpx;
  height:100rpx;
  position:absolute;
  left:0;
  top:0;
}
.familydoctor_plus{
  width:100rpx;
  height:100rpx;
  position:relative;
}
.familydoctor_plus image{
  width:40rpx;
  height:40rpx;
  position:absolute;
  left:30rpx;
  top:40rpx;
}
.familydoctor_plus{
  margin:0 auto;
}
.familydoctor_item.plus{
  padding-left:0;
  padding-right:0;
}
.familydoctor_add{
  text-align: center;
  color:#7f7b7f;
  font-size:28rpx;
  line-height:40rpx;
}