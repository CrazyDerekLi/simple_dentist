.container{
  background:#f8f8f8;
}
.familydoctor_top{
  height:420rpx;
  background:#74bee3;
  border-top:1rpx solid #9ed2ea;
  border-bottom:1rpx solid #f2f2f2;
}
.familydoctor_photo{
  width:160rpx;
  height:160rpx;
  margin:60rpx auto 20rpx auto;
  position:relative;
}
.familydoctor_photo image{
  position:absolute;
  left:0;
  right:0;
  width:160rpx;
  height:160rpx;
}
.familydoctor_name{
  color:#fff;
  text-align: center;
  height:40rpx;
  font-size:36rpx;
  font-weight: bold;
}
.btn_bao{
  width:36rpx;
  height:40rpx;
  line-height:40rpx;
  font-size:24rpx;
  display: inline-block;
  background:#f7b05d;
  border-bottom-left-radius: 36rpx;
  border-bottom-right-radius: 36rpx;
}
.familydoctor_position{
  text-align: center;
  color:#fff;
  font-size:28rpx;
  line-height:50rpx;
}
.familydoctor_btn{
  width:200rpx;
  height:50rpx;
  font-size:30rpx;
  color:#fff;
  border:1rpx solid #fff;
  line-height:50rpx;
  border-radius: 50rpx;
  margin:0 auto;
  text-align: center;
}
.familydoctor_title{
  color:#7f7b7f;
  line-height:100rpx;
  background:#fff;
  padding-left:40rpx;
  font-size:32rpx;
}
.familydoctor_item{
  position:relative;
  height:160rpx;
  padding-left:160rpx;
  font-size:32rpx;
  color:#7f7b7f;
  background: #fff;
  line-height: 160rpx;
  border-top:1rpx solid #f2f2f2;
  border-bottom:1rpx solid #f2f2f2;
  margin-bottom:20rpx;
}
.familydoctor_item_tip{
  position:absolute;
  height:160rpx;
  line-height: 160rpx;
  top:0;
  right:0;
  padding-right:40rpx;
  color:#b5b5b5;
  font-size:28rpx;
}
.familydoctor_item_photo{
  width:110rpx;
  height:110rpx;
  position:absolute;
  top:25rpx;
  left:25rpx;
}
.familydoctor_item_photo image{
  width:110rpx;
  height:110rpx;
}
.familydoctor_plus{
  width:100rpx;
  height:100rpx;
  position:relative;
}
.familydoctor_plus image{
  width:40rpx;
  height:40rpx;
  position:absolute;
  left:30rpx;
  top:40rpx;
}
.familydoctor_plus{
  margin:0 auto;
}
.familydoctor_item.plus{
  padding-left:0;
  padding-right:0;
}
.familydoctor_add{
  text-align: center;
  color:#7f7b7f;
  font-size:28rpx;
  line-height:40rpx;
}