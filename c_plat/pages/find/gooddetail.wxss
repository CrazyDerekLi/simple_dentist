.lunbo{
    height:350rpx;
    overflow:hidden;
    background:#fff;
}
.lunbo swiper{
    height:350rpx;
}
.slide_image{
  position:relative;
  display: block;
  height:350rpx;
  width:840rpx;
}
.container{
  background: #f5f5f9;
  padding-bottom:100rpx;
  z-index: 1;
}
.good_btns{
  position:fixed;
  bottom:0;
  border-top:1rpx solid #f2f2f2;
  background: #fff;
  left:0;
  right:0;
}
.good_left_btn,.good_center_btn{
  width:25%;
  height:40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size:26rpx;
  color:#ccc;
  float:left;
  padding-top:60rpx;
  position:relative;
}
.good_center_btn{
  border-left:1rpx solid #f2f2f2;
  margin-left:-1rpx;
}
.good_right_btn{
  width:50%;
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size:32rpx;
  color:#fff;
  background:#f38996;
  float:left;
  border-left:1rpx solid #f2f2f2;
  margin-left:-1rpx;
}
.good_left_btn image,.good_center_btn image{
  position:absolute;
  width:50rpx;
  height:50rpx;
  top:15rpx;
  left:50%;
  margin-left:-20rpx;
}
.good_center_btn image{
  margin-left:-25rpx;
}
.good_center_btn .tip{
  width:30rpx;
  height:30rpx;
  line-height: 30rpx;
  border-radius:30rpx;
  color:#fff;
  background:#f38996;
  text-align:center;
  position:absolute;
  top:10rpx;
  left:50%;
  margin-left:20rpx;
}
.good_info{
  border-bottom:1rpx solid #f2f2f2;
  padding:10rpx 0;
  background:#fff;
}
.good_title{
  font-size:36rpx;
  line-height: 60rpx;
  height:60rpx;
  color:#7e7b7f;
  padding-left:40rpx;
}
.good_content{
  line-height:50rpx;
  font-size:28rpx;
  color:#999;
  padding-left:40rpx;
}
.good_amount_box{
  height:60rpx;
  line-height:60rpx;
}
.good_amount{
  height:60rpx;
  line-height:60rpx;
  padding-left:40rpx;
  color:#ff6666;
  float:left;
  font-weight: bold;
}
.good_tip{
  height:60rpx;
  line-height: 60rpx;
  float:left;
  color:#999;
  padding-left:20rpx;
  font-size:28rpx;
}
.good_choose{
  height:90rpx;
  line-height:90rpx;
  position:relative;
  background:#fff;
  border-bottom:1rpx solid #f2f2f2;
}
.choose_label{
  height:90rpx;
  line-height:90rpx;
  color:#999;
  padding-left:40rpx;
  font-size:28rpx;
  float:left;
}
.choose_info{
  height:90rpx;
  line-height:90rpx;
  color:#7e7b7f;
  font-weight: bold;
  padding-left:20rpx;
  font-size:28rpx;
  float:left;
}
.choose_img{
  position:absolute;
  right:40rpx;
  width:12rpx;
  height:90rpx;
}
.choose_img image{
  position:absolute;
  left:0;
  top:33rpx;
  width:12rpx;
  height:24rpx;
}
.good_body{
  padding:20rpx 40rpx;
  font-size:28rpx;
  border-bottom: 1rpx solid #f2f2f2;
  line-height:50rpx;
  background:#fff;
  margin-top:20rpx;
  color:#7e7b7f;
  background: #fff;
}
.good_pingjia_info{
  padding:20rpx 40rpx;
  font-size:28rpx;
  color:#666;
  line-height:40rpx;
}
.good_pingjia_list{
  background:#fff;
  margin-bottom:20rpx;
}
.good_pingjia_item{
