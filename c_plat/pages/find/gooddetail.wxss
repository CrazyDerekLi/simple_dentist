  border-bottom:1rpx solid #f2f2f2;
}
.good_pingjia_user_info{
  height:100rpx;
  padding:20rpx 20rpx 0 140rpx;
  position:relative;
}
.pingjia_photo{
  position:absolute;
  left:40rpx;
  top:20rpx;
  width:80rpx;
  height:80rpx;
  border-radius:80rpx;
}
.pingjia_photo image{
  width:80rpx;
  height:80rpx;
}
.pingjia_user{
  line-height:50rpx;
  height:50rpx;
  font-size:28rpx;
  color:#666;
}
.pingjia_date{
  line-height:50rpx;
  height:50rpx;
  font-size:26rpx;
  color:#999;
}
.btn_wuyou{
  height:40rpx;
  line-height:40rpx;
  background:#ffcc66;
  color:#fff;
  font-size:28rpx;
  display: inline-block;
  padding:0 10rpx;
  border-radius: 4rpx;
  margin:0 10rpx;
}
.btn_quan{
  height:40rpx;
  line-height:40rpx;
  background:#f38996;
  color:#fff;
  font-size:28rpx;
  display: inline-block;
  padding:0 10rpx;
  border-radius: 4rpx;
  margin:0 10rpx;
}
.msk{
  position:fixed;
  left:0;
  top:0;
  right:0;
  bottom:0;
  background:#000;
  opacity: 0.3;
  z-index: 100;
  display: none;
}
.choose_box{
  position:fixed;
  left:0;
  right:0;
  bottom:0;
  padding-bottom:100rpx;
  padding-top:40rpx;
  z-index:101;
  display: none;
}
.choose_box_body{
  background:#fff;
}
.choose_good_img{
  position:absolute;
  left:40rpx;
  top:0;
  width:200rpx;
  height:200rpx;
  border:1rpx solid #f2f2f2;
  background:#fff;
  border-radius: 10rpx;
  overflow: hidden;
}
.btn_close{
  position:absolute;
  top:60rpx;
  right:20rpx;
}
.btn_close image{
  width:30rpx;
  height:30rpx;
}
.choose_count_left_btn,.choose_count_right_btn{
  width:60rpx;
  height:60rpx;
  position:absolute;
  top:0;
}
.choose_count_left_btn{
  left:0;
  border-right:1rpx solid #f2f2f2;
}
.choose_count_right_btn{
  right:0;
  border-left:1rpx solid #f2f2f2;
}
.choose_count_left_btn image,.choose_count_right_btn image{
  width:40rpx;
  height:40rpx;
  position:absolute;
  left:10rpx;
  top:10rpx;
}
.choose_count{
  position:relative;
  height:100rpx;
  background: #fff;
}
.choose_count_btns{
