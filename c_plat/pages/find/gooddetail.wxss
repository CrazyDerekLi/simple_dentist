}
.choose_btn{
  position:fixed;
  left:0;
  right:0;
  bottom:0;
  height:100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size:32rpx;
  color:#fff;
  background: #f38996;
}
.color_blue,.color_green,.color_orange{
  width:30rpx;
  height:30rpx;
