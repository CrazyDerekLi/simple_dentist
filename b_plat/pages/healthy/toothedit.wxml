      <view class='tooth_item_label'>左下</view>
      <view class='tooth_item_box'>
        <view class='tooth_item_single{{typeSelectedList[3][0]!="0"?" selectedType":""}}{{showTypePickerInfo == "3,0"?" selected":""}}' bindtap='showTypePicker' data-index='3,0' style='{{typeSelectedList[3][0]!="0"?"background:"+typeList[typeSelectedList[3][0]].typeColor:""}}'>1</view>
