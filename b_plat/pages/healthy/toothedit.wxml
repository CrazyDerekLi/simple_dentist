        <view class='tooth_item_single{{typeSelectedList[3][2]!="0"?" selectedType":""}}{{showTypePickerInfo == "3,2"?" selected":""}}' bindtap='showTypePicker' data-index='3,2' style='{{typeSelectedList[3][2]!="0"?"background:"+typeList[typeSelectedList[3][2]].typeColor:""}}'>3</view>
        <view class='tooth_item_single{{typeSelectedList[3][3]!="0"?" selectedType":""}}{{showTypePickerInfo == "3,3"?" selected":""}}' bindtap='showTypePicker' data-index='3,3' style='{{typeSelectedList[3][3]!="0"?"background:"+typeList[typeSelectedList[3][3]].typeColor:""}}'>4</view>
        <view class='tooth_item_single{{typeSelectedList[3][4]!="0"?" selectedType":""}}{{showTypePickerInfo == "3,4"?" selected":""}}' bindtap='showTypePicker' data-index='3,4' style='{{typeSelectedList[3][4]!="0"?"background:"+typeList[typeSelectedList[3][4]].typeColor:""}}'>5</view>
        <view class='tooth_item_single{{typeSelectedList[3][5]!="0"?" selectedType":""}}{{showTypePickerInfo == "3,5"?" selected":""}}' bindtap='showTypePicker' data-index='3,5' style='{{typeSelectedList[3][5]!="0"?"background:"+typeList[typeSelectedList[3][5]].typeColor:""}}'>6</view>
