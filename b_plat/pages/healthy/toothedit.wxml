  <view class='tooth_bottom_btn' bindtap='saveAndGoBack'>
    确认提交
  </view>

</view>

<view class="tooth_type_picker" style="display:{{showTypePicker?'block':'none'}};" >
  <view class='tooth_type_title'>{{selectedRowName+selectedToothName}}</view>
  <picker-view indicator-style="height: 40px;" class="tooth_type_picker_container" value="{{typeIndex}}" bindchange="bindChangeType">
    <picker-view-column>
      <view class="tooth_type_picker_item" wx:for="{{typeList}}" style='background:{{item.typeColor}}'>
        {{item.typeName}}
      </view>
    </picker-view-column>
  </picker-view>
  <view class="btn_cancel" bindtap='hidePicker'>返回</view>
</view>